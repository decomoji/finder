{
  "name": "decomoji-finder",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "postinstall": "node build_decomoji_module.js && npm run copy-decomoji",
    "copy-decomoji": "cpx 'node_modules/slack-reaction-decomoji/decomoji/{basic,extra,explicit}/*.png' public/decomoji/",
    "update-decomoji": "cd node_modules/slack-reaction-decomoji && git pull origin master && cd - && npm run postinstall"
  },
  "dependencies": {
    "@maboroshi/math-utils": "^1.4.3",
    "@maboroshi/selector": "^1.0.3",
    "@maboroshi/shoshiki": "^1.2.2",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-polyfill": "^6.26.0",
    "cpx": "^1.5.0",
    "slack-reaction-decomoji": "github:decomoji/slack-reaction-decomoji",
    "vue": "^2.6.6",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.18",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-type-helper": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/parser": "^1.10.2",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-jest": "^4.0.3",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.13.0",
    "eslint-plugin-vue": "^5.2.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser": "^4.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
