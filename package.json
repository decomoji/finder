{
  "name": "decomoji-finder",
  "version": "3.3.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "update:decomoji": "rm -rf node_modules/decomoji && npm i -d decomoji/decomoji && npm run postinstall",
    "postinstall": "cpx 'node_modules/decomoji/decomoji/{basic,extra,explicit}/*.png' public/decomoji/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/vue-virtual": "^3.8.6",
    "decomoji": "github:decomoji/decomoji",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.2.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "cpx": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "tailwindcss": "^3.4.9",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vue-tsc": "^2.0.29"
  },
  "volta": {
    "node": "20.11.1"
  }
}
