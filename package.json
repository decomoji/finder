{
  "name": "decomoji-finder",
  "version": "2.10.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "update:decomoji": "rm -rf node_modules/decomoji && npm i -d decomoji/decomoji && npm run postinstall",
    "postinstall": "cpx 'node_modules/decomoji/decomoji/{basic,extra,explicit}/*.png' public/decomoji/"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-polyfill": "^6.26.0",
    "cpx": "^1.5.0",
    "decomoji": "github:decomoji/decomoji",
    "normalize.css": "^8.0.1",
    "npm": "^6.14.7",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.4.0",
    "vue-router": "^3.1.5",
    "vue-virtual-scroller": "^1.0.10",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "vuex-type-helper": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/parser": "^1.10.2",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "commitizen": "^4.2.1",
    "cz-customizable": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-plugin-vue": "^5.2.1",
    "husky": "^4.2.5",
    "jest": "^24.8.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^8.0.2",
    "terser": "^4.6.3",
    "terser-webpack-plugin": "^1.4.3",
    "ts-jest": "^26.0.0",
    "typescript": "^3.7.5",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
